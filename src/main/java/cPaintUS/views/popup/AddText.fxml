<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.*?>

<AnchorPane minHeight="192.0" minWidth="300.0"
	prefHeight="192.0" prefWidth="300.0"
	xmlns:fx="http://javafx.com/fxml/1"
	xmlns="http://javafx.com/javafx/2.2"
	fx:controller="cPaintUS.controllers.AddTextController">
	<VBox alignment="CENTER" minHeight="300.0" spacing="10.0">
		<HBox fx:id="buttonsHBox" alignment="CENTER" spacing="10.0" />
	</VBox>
	<TextField id="AddText" fx:id="addText" layoutX="14.0"
		layoutY="77.0" prefHeight="39.0" prefWidth="272.0" />
	<Button fx:id="cancelBtn" layoutX="157.0" layoutY="140.0"
		onAction="#handleCancelClick" text="Cancel" />
	<Button fx:id="addBtn" layoutX="65.0" layoutY="140.0"
		onAction="#handleAddTextClick" prefWidth="56.0" text="Add" />
	<Label layoutX="109.0" layoutY="22.0" text="Add text"
		textAlignment="CENTER">
		<font>
			<Font size="15.0" />
		</font>
	</Label>
</AnchorPane>
